{
  "source": {
    "include": [
      "src",
      "package.json",
      "README.md"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins":[
    "plugins/markdown"
  ],
  "opts": {
    "template": "./node_modules/minami",
    "destination": "./docs/"
  }
}